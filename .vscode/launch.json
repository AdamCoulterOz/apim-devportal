  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/source/index.ts",
      "args": [
        "/subscriptions/000000/resourceGroups/xxxxx/providers/Microsoft.ApiManagement/service/yyyyy",
        "--export",
        "--delete",
        "--import"
      ],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/build/**/*.js"]
    }
  ]
}
